	"version": "0.2.0",
	"configurations": [
		{
			"name": "Dump whatever's connected",
			"type": "python",
			"request": "launch",
			"module": "pm2hw",
			"cwd": "${workspaceFolder}",
			"env": {"PYTHONPATH": "${workspaceFolder}:${env:PYTHONPATH}"},
			"args": ["-d", "test_dump.min"]
		},
		{
			"name": "Flash test_flash.min to whatever's connected",
			"type": "python",
			"request": "launch",
			"module": "pm2hw",
			"cwd": "${workspaceFolder}",
			"env": {"PYTHONPATH": "${workspaceFolder}:${env:PYTHONPATH}"},
			"args": ["-f", "test_flash.min"]
		},
	]
}
